programa {
  funcao inicio() {
    escreva("Digite quantos litros você quer: ")
    real gasolina
    leia(gasolina)
    escreva("Valor do litro da gasolina: ")
    real litro
    leia(litro)
    real multiplicacao = gasolina * litro
    escreva("Valor total do abastecimento: ", multiplicacao)
  }
}
